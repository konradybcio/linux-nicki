/dts-v1/;

#include "msm8956-sony-xperia-loire.dtsi"

/ {
	model = "SoMC Loire Suzu-ROW";
	compatible = "somc,suzu-row", "qcom,msm8956";

	qcom,board-id = <8 0>;
};

&blsp_i2c8 {
	status = "okay";
};

&usb_hs_phy {
	qcom,init-seq = /bits/ 8 <0x0 0x74 0x1 0x6f 0x2 0x3f 0x3 0x33>;
};

